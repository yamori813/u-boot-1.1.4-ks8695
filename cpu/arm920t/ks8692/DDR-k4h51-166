	mov	r2, #0x00200000		/* 36 - 0x00200020 */
	add	r2, r2, #0x00000020
	str	r2, [r1], #4

	mov	r2, #0x05000000		/* 37 - 0x050e0000 */
	add	r2, r2, #0x000e0000
	str	r2, [r1], #4

	mov	r2, #0x00002d00		/* 38 - 0x00002d89 */
	add	r2, r2, #0x00000089
	str	r2, [r1], #4

	mov	r2, #0x00000000		/* 39 - 0x00000000 */
	str	r2, [r1], #4

	mov	r2, #0x00000000		/* 40 - 0x00000000 */
	str	r2, [r1], #4

	mov	r2, #0x00000000		/* 41 - 0x00000000 */
	str	r2, [r1], #4

	mov	r2, #0x0000c800		/* 42 - 0x0000c800 */
	str	r2, [r1], #4

	mov	r2, #0x00c80000		/* 43 - 0x00c8000d */
	add	r2, r2, #0x0000000d
	str	r2, [r1], #4

	mov	r2, #0x00000022		/* 44 - 0x00000022 */
	str	r2, [r1], #4

	mov	r1, #0x1f000000		/* KS8692_IO_BASE+0x4038 */
	add	r1, r1, #0x00ff0000
	add	r1, r1, #0x00004000
	add	r1, r1, #0x00000038

	mov	r2, #0x00000001		/* start DDR */
	str	r2, [r1]

	/* end of DDR Initialization */

